class Displaying {
    public statuc void main (String args [])
    {
        System.out.println("Screen Display");
        for(i=1;i<=9; i++)
        {
            for(j=1;j<=i;j++)
            {
                System.out.println( " " );
                System.out.println( i );
            }
            System.out.println("\n");
        }
        System.out.println("Screen display done");
    }
}